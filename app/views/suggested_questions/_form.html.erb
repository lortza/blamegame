<%= form_with(model: suggested_question, local: true) do |form| %>
  <%= render 'shared/errors', object: suggested_question %>

  <div class="field">
    <%= form.text_field :text, class: 'input' %>
  </div>

  <div class="actions">
    <%= form.submit 'Submit', class: button_classes %>
  </div>
<% end %>
